        <div class="container" style="margin-top: 50px;">
            <table class="table table-striped table-hover ">
            <thead>
                <tr class="info">
                    <th> Id </th>
                    <th> First Name </th>
                    <th> Last Name </th>
                    <th> User Name </th>
                    <th> Email </th>
                    <th> Password </th>
                </tr>
                </thead>
                <tbody>
                <% for(var i=0; i<admin.length; i++ ){ %>
                <tr class="warning">
                    <td><%= admin[i]._id  %></td>
                    <td><%= admin[i].FirstName%></td>
                    <td><%= admin[i].LastName%></td>
                    <td><%= admin[i].UserName%></td>
                    <td><%= admin[i].Email%></td>
                    <td><%= admin[i].Password%></td>    
                </tr>
                <% } %>
            </tbody>
            </table>
